    "type": "custom-installer",
    "keywords": ["phantomjs", "headless", "binaries"],
    "license": "MIT",
    "version": "1.9.7",
    "authors": [
        {
            "name": "Jens-Andr√© Koch",
            "email": "jakoch@web.de"
        }
    ],
    "require": {
        "ext-openssl": "*"
    },
	"require-dev": {
		"composer/composer": "1.0.*@dev"
	},
    "autoload": {
        "psr-0": {
            "PhantomInstaller\\": "src/"
        }
    }
}